{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-dynamic-require": "off",
        "class-methods-use-this": "off",
        "max-len": ["error", {"code": 120, "ignoreComments": true}]
    }
}
